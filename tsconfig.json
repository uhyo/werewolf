{
    "compilerOptions":{
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "strictNullChecks": true,
        "module":"commonjs",
        "target":"es5",
        "outDir":"dist/"
    },
    "exclude":[
        "node_modules"
    ]
}



